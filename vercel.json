{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/api/bot.py"
    }
  ],
  "functions": {
    "api/bot.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "TELEGRAM_BOT_TOKEN": "@telegram_bot_token"
  }
}